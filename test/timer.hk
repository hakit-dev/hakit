[objects]
input1: sink local
input2: sink local widget=switch-push
input3: sink
input: or ninputs=3
pulse: timer-pulse delay=3000
pulse-output: source
on: timer-on delay=1000
on-output: source
off: timer-off delay=5000
off-output: source

[nets]
input1.out input.in0
input2.out input.in1
input3.out input.in2
input.out pulse.in on.in off.in
pulse.out pulse-output.in
on.out on-output.in
off.out off-output.in
